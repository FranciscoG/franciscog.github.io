---
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<script webc:setup>
	// somehow the page.url for the /tags/ page is a regex object
	// so we need to convert it to a string
	const all = collections.all.map(_page => {
		_page.url = _page.url.toString();
		return _page;
	});
</script>
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="http://www.w3.org/1999/xhtml">
<url webc:for="pageData of all">
	<loc @html="htmlBaseUrl(pageData.url, metadata.url)"></loc>
	<lastmod @html="htmlDateString(pageData.date)"></lastmod>
</url>
</urlset>
